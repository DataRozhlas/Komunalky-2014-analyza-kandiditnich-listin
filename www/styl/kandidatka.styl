.ig
  &.kandidatka-active .kandidatka-wrap
    left 0px !important

  .kandidatka-wrap
    position absolute
    top 0px
    left -100%
    width 100%
    bottom 0px
    padding-right 340px
    z-index 2000
    // transition left 0.8s
    .kandidatka
      width 100%
      height 100%
      overflow auto
      background white
      h1
        font-size 1.8em
        color ColorHeadingLink
        margin 0px
        padding 16px 15px
      table.data-table
        border-collapse collapse
        width 100%
        td
        th
          margin 0px
        td
          padding 2px 5px
        th
          background ColorRadiozurnal
          border-bottom 1px solid darken(ColorRadiozurnal, 20%)
          color white
          padding 5px 5px
          text-align left
        td:nth-child(4)
        th:nth-child(4)
          height 42px
        tbody
          tr
            td
              text-overflow ellipsis
            &:nth-child(2n) td
              background #f2f2f2





